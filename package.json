{
  "name": "portfolio",
  "version": "0.1.0",
  "homepage": "https://chammounc.github.io",
  "description": "Portfolio site for Chammoun Chakhap",
  "repository": "https://github.com/Chammounc/portfolio.git",
  "author": "Chammoun Chakhap",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@hamishmw/react-scripts-postcss": "4.0.0-mod1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "enzyme": "^3.11.0",
    "gh-pages": "^6.0.0",
    "mdx-loader": "^3.0.2",
    "prettier": "^2.8.8",
    "react-snap": "1.23.0",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^15.9.0",
    "stylelint-config-recommended": "^12.0.0"
  },
  "dependencies": {
    "@mdx-js/react": "^2.3.0",
    "@react-email/render": "^0.0.7",
    "classnames": "^2.3.2",
    "popmotion": "^8.7.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "^5.0.1",
    "react-scripts-postcss": "^4.0.0-mod1",
    "react-transition-group": "^4.4.5",
    "three": "^0.126.0"
  },
  "scripts": {
    "predeploy":"npm run build",
    "deploy":"gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "prettier-format": "prettier --config .prettierrc \"src/**/*.js\" --write"
  },
  "reactSnap": {
    "skipThirdPartyRequests": true,
    "headless": true,
    "crawl": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "browserslist": {
    "production": [
      ">10%",
      "not dead",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
